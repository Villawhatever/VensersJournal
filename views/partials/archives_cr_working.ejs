<div class='d-flex flex-column w-100' style='background:red;'>
  <% mcr.forEach(function(each, index) { %>
    <div class='card my-1' >
      <a href='/static/rules/<%= each.filepath %>' target='_blank'>
        <i class='ss ss-<%- each.filepath.substr(-7, 3).toLowerCase() %>' style='font-size:1.75em; float:left;'></i>
        <span><%- each.setName %> (<%- each.filepath.substr(-7, 3) %>)</span>
      </a>  
    </div>
  <% }); %>
  
</div>